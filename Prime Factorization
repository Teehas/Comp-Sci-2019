<?php

$N=1389;
$i=2;

while ($i<$N) {
$TEMP = $N/$i;
if(is_int($TEMP))
{
echo $i;
$i=2;
$N=$TEMP;
  }
}
?>

<?php
$N=10;
$i=2;
$count=0;
for ($i = 2; $i<=sqrt($N) ;$i++) 
{
    $TEMP = $N/$i;
    if(is_int($TEMP))
    {
        echo $i;
        echo "<br>";
        $i = 1;
        $N=$TEMP;
    }
    $count++;
}
    echo $count;
?>
